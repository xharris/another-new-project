{
  "name": "BlankE",
  "description": "i use this to make games now",
  "repository": {
    "type": "git",
    "url": "git://github.com/xharris/blanke4.git"
  },
  "main": "main.js",
  "author": "Xavier Harris <xhh950@gmail.com> (http://xhhstuff.me)",
  "license": "CC-BY-NC-SA-4.0",
  "scripts": {
    "start": "grunt && electron . --dev",
    "release": "grunt && npm run clean_modules && npm run asar && npm install",
    "asar": "asar pack \"../src\" \"../releases/BlankE-win32-ia32/resources/app.asar\" --unpack-dir \"**/engines/**\"",
    "clean_modules": "npm prune --production"
  },
  "webkit": {
    "plugin": true
  },
  "dependencies": {
    "codemirror": "^5.25.0",
    "cryptr": "^2.0.0",
    "electron-cookies": "^1.1.0",
    "fs-extra": "^0.26.7",
    "getmac": "^1.2.1",
    "hazardous": "^0.2.3",
    "image-size": "^0.4.0",
    "less": "^2.7.1",
    "mdi": "^1.6.50",
    "mkdirp": "^0.5.1",
    "open": "0.0.5",
    "replace-in-file": "^2.3.0",
    "rimraf": "^2.5.4"
  },
  "devDependencies": {
    "electron": "0.4.1",
    "grunt": "^1.0.1",
    "grunt-contrib-less": "^1.3.0"
  },
  "build": {
    "appID": "gamething",
    "app-category-type": "public.app-category.developer-tools",
    "win": {
      "iconURL": "https://raw.githubusercontent.com/xharris/texteditor/master/src/build/icon.ico"
    }
  }
}
